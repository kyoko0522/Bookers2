<div class="row">
    <div class="col-md-3">
<%= render 'layouts/list',book: @book, user: @user %>
</div>
<div class="col-md-8 offset-md-1">
<h1>Users</h1>
<table class="table table-hover table-inverse">
<% @users.each do |user| %>

<ul><%= attachment_image_tag user, :profile_image, :fill, 30, 30, fallback: "no_image.jpg"%></ul>
<ul><%= user.name %></ul>
<ul><%= link_to'Show', user_path(user) %></ul>
<% end %>
</table>
</div>
</div>