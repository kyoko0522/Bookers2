<div class="container ">
<div class="row">
    <div class="col-md-3">
<%= render 'layouts/list',book: @book, user: @user %>
</div>
<div class="col-md-8 offset-md-1">
<h1>Users</h1>
  <table class="table table-hover table-inverse">
    <thead>
        <tr>
        <th>image</th>
        <th>name</th>
        <th colspan="3"></th>
        </tr>
    </thead>
  <% @users.each do |user| %>
  <tr>
    
    <td><%= attachment_image_tag user, :profile_image, :fill, 30, 30, fallback: "no_image.jpg"%></td>
    <td><%= user.name %></td>
    <td><%= link_to'Show', user_path(user) %></td>
  </tr>
  <% end %>
  </table>
</div>
</div>
</div>